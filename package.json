{
  "name": "cleanlog-ui",
  "version": "1.0.0",
  "main": "src/index.js",
  "type": "module",
  "jest": {
    "testEnvironment": "node",
    "verbose": true,
    "testMatch": [
      "**/__tests__/**/*.test.js",
      "**/**/**/*.test.js"
    ]
  },
  "scripts": {
    "start": "node src/index.js",
    "watch": "NODE_ENV=test node --watch-path ./src --watch-path ./public src/index.js",
    "tailwind-watch": "npx tailwindcss -i src/input.css -o ./public/css/output.css -w",
    "test": "NODE_OPTIONS=--experimental-vm-modules npx jest",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules npx jest --watch",
    "pretest": "npm run lint",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build": "NODE_OPTIONS=--experimental-vm-modules npx tailwindcss -i src/input.css -o ./public/css/output.css --minify"
  },
  "author": "Jamie Nuttall",
  "license": "MIT",
  "description": "",
  "devDependencies": {
    "eslint": "^9.9.1",
    "get-port": "^7.1.0",
    "jest": "^29.7.0",
    "puppeteer": "^23.1.1",
    "tailwindcss": "^3.4.10"
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "eventsource": "^2.0.2",
    "express": "^4.21.0",
    "express-es6-template-engine": "^2.2.3",
    "express-session": "^1.0.0",
    "google-auth-library": "^9.14.0",
    "http-proxy": "^1.18.1",
    "nats": "^2.28.2",
    "pocketbase": "^0.22.1",
    "winston": "^3.14.2"
  }
}
